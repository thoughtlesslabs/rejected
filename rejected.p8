function _init()
	-- init ai players
	comp={
	 {name="jimbo",x=80,y=100,
	 	h=8,w=8,dx=0,dy=0,g=0.5,
	 	f=0.7,j=false,jh=25,boost=6},
	 {name="tank",x=20,y=100,
	 	h=8,w=8,dx=0,dy=0,g=0.5,
	 	f=0.9,j=false},jh=20}
	-- init player
	player={
		name="you",x=5,y=60,
		h=8,w=8,dx=0,dy=0,g=0.5,
		f=0.7,j=false}
	-- init ball
	ball={
		x=60,y=10,dx=0,dy=0,r=4,
		g=0.05,f=0.98}
	
	rims={
		{name="left",x=3,y=85,
			h=1,w=8},
		{name="right",x=117,y=85,
			h=1,w=8}}
	
	
	-- defaults
	shake = 0 
	current = 1
	debug = ""
end

function _update60()
	screenshake()
	moveplayer()
	moveai(ball.x,ball.y)
	moveball(player.x,player.y)
end

function _draw()
	cls()
	drawbackground()
	print(debug)
--	print(debug2)
--	print(debug3)
	spr(1,player.x,player.y)
	spr(2,comp[current].x,comp[current].y)
