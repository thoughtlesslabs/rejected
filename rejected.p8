function screenshake()
	local shakex = 16-rnd(32)
	local shakey = 16-rnd(32)

	camx = shakex*shake
	camy = shakey*shake
	camera(camx,camy)
	
	shake=shake*0.95
	if shake<0.05 then
		shake=0
	end
end
